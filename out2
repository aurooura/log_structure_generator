#define LOG_AD_104_BASE(struct_enum)\
	{ (struct_enum), sizeof(log_AD_104), "A104",\
	"QiifB","t,1,2,3,4","s----","F0000"}

struct PACKED log_AD_104 {
	LOG_PACKET_HEADER;
	uint64_t time_us;
	int32_t stateIndexLim;
	int32_t PV_AidingMode;
