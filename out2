===============LOG_FILE_CPP====================

void AP_Logger::Write_alignMagStateDeclination100(
	float statesArray_0,
	float statesArray_1,
	float statesArray_2,
	float statesArray_3,
	float statesArray_4,
	float statesArray_5,
	float statesArray_6,
	float statesArray_7,
	float statesArray_8,
	float statesArray_9,
	float statesArray_10,
	float statesArray_11,
	float statesArray_12,
	float statesArray_13,
	float statesArray_14)
{
	struct log_AD_100 pkt = {
		LOG_PACKET_HEADER_INIT(LOG_AD_100),
		time_us : AP_HAL::micros64(),
		statesArray_0 : statesArray_0,
		statesArray_1 : statesArray_1,
		statesArray_2 : statesArray_2,
		statesArray_3 : statesArray_3,
		statesArray_4 : statesArray_4,
		statesArray_5 : statesArray_5,
		statesArray_6 : statesArray_6,
		statesArray_7 : statesArray_7,
		statesArray_8 : statesArray_8,
		statesArray_9 : statesArray_9,
		statesArray_10 : statesArray_10,
		statesArray_11 : statesArray_11,
		statesArray_12 : statesArray_12,
		statesArray_13 : statesArray_13,
		statesArray_14 : statesArray_14
	};
	WriteBlock(&pkt, sizeof(pkt));
} 

void AP_Logger::Write_alignMagStateDeclination101(
	float statesArray_15,
	float statesArray_16,
	float statesArray_17,
	float statesArray_18,
	float statesArray_19,
	float statesArray_20,
	float statesArray_21,
	float statesArray_22,
	float statesArray_23,
	float statesArray_24,
	float statesArray_25,
	float statesArray_26,
	float statesArray_27,
	float statesArray_0_out,
	float statesArray_1_out)
{
	struct log_AD_101 pkt = {
		LOG_PACKET_HEADER_INIT(LOG_AD_101),
		time_us : AP_HAL::micros64(),
		statesArray_15 : statesArray_15,
		statesArray_16 : statesArray_16,
		statesArray_17 : statesArray_17,
		statesArray_18 : statesArray_18,
		statesArray_19 : statesArray_19,
		statesArray_20 : statesArray_20,
		statesArray_21 : statesArray_21,
		statesArray_22 : statesArray_22,
		statesArray_23 : statesArray_23,
		statesArray_24 : statesArray_24,
		statesArray_25 : statesArray_25,
		statesArray_26 : statesArray_26,
		statesArray_27 : statesArray_27,
		statesArray_0_out : statesArray_0_out,
		statesArray_1_out : statesArray_1_out
	};
	WriteBlock(&pkt, sizeof(pkt));
} 

