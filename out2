} 

void AP_Logger::Write_alignMagStateDeclination103(
	float statesArray_17_out,
	float statesArray_18_out,
	float statesArray_19_out,
	float statesArray_20_out,
	float statesArray_21_out,
	float statesArray_22_out,
	float statesArray_23_out,
	float statesArray_24_out,
	float statesArray_25_out,
	float statesArray_26_out,
	float statesArray_27_out,
	uint8_t magFieldLearned,
	uint8_t magDecAng,
	uint8_t inhibitMagStates)
{
	struct log_AD_103 pkt = {
		LOG_PACKET_HEADER_INIT(LOG_AD_103),
		time_us : AP_HAL::micros64(),
		statesArray_17_out : statesArray_17_out,
		statesArray_18_out : statesArray_18_out,
		statesArray_19_out : statesArray_19_out,
		statesArray_20_out : statesArray_20_out,
		statesArray_21_out : statesArray_21_out,
		statesArray_22_out : statesArray_22_out,
		statesArray_23_out : statesArray_23_out,
		statesArray_24_out : statesArray_24_out,
		statesArray_25_out : statesArray_25_out,
		statesArray_26_out : statesArray_26_out,
		statesArray_27_out : statesArray_27_out,
		magFieldLearned : magFieldLearned,
		magDecAng : magDecAng,
		inhibitMagStates : inhibitMagStates
	};
	WriteBlock(&pkt, sizeof(pkt));
} 

void AP_Logger::Write_alignMagStateDeclination104(
	int32_t stateIndexLim,
	int32_t PV_AidingMode,
	float dtEkfAvg,
	uint8_t faultStatus_bad_decl)
{
	struct log_AD_104 pkt = {
		LOG_PACKET_HEADER_INIT(LOG_AD_104),
		time_us : AP_HAL::micros64(),
		stateIndexLim : stateIndexLim,
		PV_AidingMode : PV_AidingMode,
		dtEkfAvg : dtEkfAvg,
		faultStatus_bad_decl : faultStatus_bad_decl
	};
	WriteBlock(&pkt, sizeof(pkt));
} 

