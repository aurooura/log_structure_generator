=============== XMOBOTS_LOGS.H ====================

#define LOG_C502_BASE(struct_enum)\
	{ (struct_enum), sizeof(log_C502), "C502",\
	"Qff","t,1,2","s--","F00"}

struct PACKED log_C502 {
	LOG_PACKET_HEADER;
	uint64_t time_us;
	float eulerAngles_x;
	float eulerAngles_y;
};

=============== AP_LOGGER_H====================

void Write_controlMagYawReset502(float eulerAngles_x,float eulerAngles_y);


