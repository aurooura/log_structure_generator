LOG_C502,
LOG_C502_BASE(LOG_C502),\


===============LOG_FILE_CPP====================

void AP_Logger::Write_controlMagYawReset502(
	float eulerAngles_x,
	float eulerAngles_y)
{
	struct log_C502 pkt = {
		LOG_PACKET_HEADER_INIT(LOG_C502),
		time_us : AP_HAL::micros64(),
		eulerAngles_x : eulerAngles_x,
		eulerAngles_y : eulerAngles_y
	};
	WriteBlock(&pkt, sizeof(pkt));
} 

